{
  "name": "ng2-vizabi-base",
  "private": false,
  "version": "4.0.7",
  "description": "angular2 vizabi component",
  "scripts": {
    "lite-server": "lite-server -c demo/bs-config.json",
    "demo.serve": "run-s build link demo.build lite-server",
    "demo.gh-pages": "run-s build demo.build demo.deploy",
    "cp-barrankchart-dist": "cp -r ./node_modules/vizabi-barrankchart/build/* ./demo/dist",
    "cp-bubblechart-dist": "cp -r ./node_modules/vizabi-bubblechart/build/* ./demo/dist",
    "cp-bubblemap-dist": "cp -r ./node_modules/vizabi-bubblemap/build/* ./demo/dist",
    "cp-linechart-dist": "cp -r ./node_modules/vizabi-linechart/build/* ./demo/dist",
    "cp-mountainchart-dist": "cp -r ./node_modules/vizabi-mountainchart/build/* ./demo/dist",
    "cp-barrankchart-src": "cp -r ./node_modules/vizabi-barrankchart/build/* ./demo/src",
    "cp-bubblechart-src": "cp -r ./node_modules/vizabi-bubblechart/build/* ./demo/src",
    "cp-bubblemap-src": "cp -r ./node_modules/vizabi-bubblemap/build/* ./demo/src",
    "cp-linechart-src": "cp -r ./node_modules/vizabi-linechart/build/* ./demo/src",
    "cp-mountainchart-src": "cp -r ./node_modules/vizabi-mountainchart/build/* ./demo/src",
    "demo.build": "ng build -prod --aot && cp -r ./node_modules/vizabi/build/* ./demo/dist && npm run cp-barrankchart-dist && npm run cp-bubblechart-dist && npm run cp-bubblemap-dist && npm run cp-linechart-dist && npm run cp-mountainchart-dist && cp ./node_modules/vizabi-ddfcsv-reader/dist/vizabi-ddfcsv-reader.js ./demo/dist",
    "demo.deploy": "gh-pages -d demo/dist",
    "link": "ngm link -p src --here",
    "lint": "exit 0",
    "disable-lint": "tslint \"**/*.ts\" -c tslint.json --fix --type-check -t prose -e \"node_modules/**\" -e \"dist/**\" -e \"temp/**\" -e \"scripts/docs/**\"",
    "flow.changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "flow.github-release": "conventional-github-releaser -p angular",
    "build": "cp -r ./node_modules/vizabi/build/* ./demo/src && npm run cp-barrankchart-src && npm run cp-bubblechart-src && npm run cp-bubblemap-src && npm run cp-linechart-src && npm run cp-mountainchart-src && cp ./node_modules/vizabi-ddfcsv-reader/dist/vizabi-ddfcsv-reader.js ./demo/src && cp ./fix/webpack-build-common.js ./node_modules/@angular/cli/models && ngm build -p src --clean && npm run link",
    "build.watch": "ngm build -p src --watch --skip-bundles",
    "start": "ng serve --aot",
    "pretest": "run-s lint build link",
    "test": "ng test -sr",
    "test-coverage": "ng test -sr -cc",
    "version": "npm run flow.changelog && git add -A"
  },
  "main": "bundles/ng2-vizabi.umd.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "keywords": [
    "angular",
    "angular2",
    "angularjs",
    "vizabi"
  ],
  "author": "Vyacheslav Chub <vyacheslav.chub@valor-software.com>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/vizabi/ng2-vizabi.git"
  },
  "bugs": {
    "url": "https://github.com/vizabi/ng2-vizabi/issues"
